    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "babel-loader": "^8.2.1",
    "chromatic": "^5.3.0",
    "jest": "^26.6.3",
    "react-hot-loader": "^4.13.0",
    "react-is": "^17.0.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
